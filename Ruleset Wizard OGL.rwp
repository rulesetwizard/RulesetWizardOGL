end

function onSourceValue(source, sourcename)

	if sourcename == "abilitybonus" then
		if window.ACStat.getValue() ~= "" then
			-- get the selected ability bonus
			return 10 + DB.getValue(window.getDatabaseNode(), ".abilities." .. window.ACStat.getValue() .. ".bonus", 0);
		else
			return 10;
		end
	end

	return super.onSourceValue(source, sourcename);
end</LUACode>
