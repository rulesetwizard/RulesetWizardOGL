    <LUACode>function onInit()
	--Initialize skill list if it's empty
    local ListNode = skills.getDatabaseNode();
    if ListNode.getChildCount() == 0 then
        for k, v in pairs(OGLData.getSkills()) do
            local nodeSkill = ListNode.createChild();
            DB.setValue(nodeSkill, "skillName", "string", k);
            DB.setValue(nodeSkill, "skillStat", "string", v["stat"]);
        end
    end
end

function UpdateSkills()
	-- Update every skill in list (called when any ability total changes)
	for _,w in ipairs(skills.getWindows()) do
		w.UpdateTotal();
	end
end</LUACode>
