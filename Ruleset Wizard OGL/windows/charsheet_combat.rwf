    <LUACode>
function onDrop(x, y, draginfo)
	return handleDrop(draginfo, nil);
end

function handleDrop(draginfo, nodeTargetList)
	if draginfo.isType("shortcut") then
		local sClass, sRecord = draginfo.getShortcutData();
		if sClass == "weapon" then
			local nodeSource = ManagerOGL.resolveRefNode(sRecord);
			if not nodeSource then
				return;
			end
			local nodeList = getDatabaseNode();
			local nodeEntry = nodeList.createChild();

			local sClass, sRecord = draginfo.getShortcutData();
			DB.setValue(nodeEntry, "linkAttack", "windowreference", sClass, sRecord);
			DB.setValue(nodeEntry, "attackName", "string", DB.getValue(nodeSource, "name"));
			DB.setValue(nodeEntry, "damageDice", "dice", DB.getValue(nodeSource, "weaponDamage"));
			DB.setValue(nodeEntry, "damageItem", "number", DB.getValue(nodeSource, "weaponDamagemodifier")); -- From dieFieldModifier template
			DB.setValue(nodeEntry, "attackProperties", "string", DB.getValue(nodeSource, "weaponProperties"));

			return true;
		end
	end
end

local sFocus = "attackName"; -- From char_attack

function onListChanged()
	update();
end

function update()
	local sEdit = getName() .. "_iedit";
	if window[sEdit] then
		local bEdit = (window[sEdit].getValue() == 1);
		for _,w in ipairs(getWindows()) do
			w.idelete.setVisibility(bEdit);
		end
	end
end

function onClickDown(button, x, y)
	if not isReadOnly() and getDatabaseNode().isOwner() then
		return true;
	end
end

function onClickRelease(button, x, y)
	if not isReadOnly() and getDatabaseNode().isOwner() then
		if getWindowCount() == 0 then
			addEntry(true);
		end
		return true;
	end
end

function addEntry(bFocus)
	local w = createWindow();
	if bFocus then
		w[sFocus].setFocus();
	end
